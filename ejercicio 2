#include <iostream>
#include <string>
using namespace std;

int main(){
    string txt, sinEsp="";
    cout << "Ingrese texto: ";
    getline(cin, txt);

    for(int i = 0; i < txt.length(); i++){
        if(txt[i] != ' '){
            sinEsp = sinEsp + txt[i];
        }
    }

    cout << sinEsp;
    return 0;
}
